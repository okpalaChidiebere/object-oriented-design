COMPILER = CC
DRIVER =TestEmployeeList
FILE1 = Employee
FILE2=IndexList
\$(DRIVER) : \$(FILE1).o \$(DRIVER).o	  				                      \$(FILE2).o \$(DRIVER).o
          $(COMPILER) -o \$(DRIVER) \$(FILE1).o \$(FILE2).o \$(DRIVER).o
$(DRIVER).o : \$(DRIVER).cpp \$(FILE1).cpp \$(FILE1).h \$(FILE2).cpp \$(FILE2).h
	\$(COMPILER) -c \$(DRIVER).cpp -o \$(DRIVER).o
\$(FILE).o : \$(FILE1).cpp \$(FILE1).h
	\$(COMPILER) -c \$(FILE1).cpp
\$(FILE).o : \$(FILE2).cpp \$(FILE2).h
	\$(COMPILER) -c \$(FILE2).cpp

clean:
	@ /bin/rm -f *.o
