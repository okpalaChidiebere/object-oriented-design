#ifndef Employee_h
#define Employee_h
#include<iostream>
#include<string>
using namespace std;

class employee
{
 private:
  string empName;
  string phoneNum;
 public:
  employee();
  void setName();
  void setPhone();
  void displayName();
  void displayPhone();
 
   friend ostream& operator<<(ostream&,const employee&);
    bool operator >(const employee&)const;
};
#endif
